    <!-- Customized Bootstrap Stylesheet -->
    <link href="~/contents/khachhang/css/bootstrap.min.css" rel="stylesheet">

    <!-- Template Stylesheet -->
    <link href="~/contents/khachhang/css/style.css" rel="stylesheet">
</head>
