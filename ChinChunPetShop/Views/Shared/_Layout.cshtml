    <!-- Topbar Start -->
    <div class="container-fluid border-bottom d-none d-lg-block">
        <div class="row gx-0">
            <div class="col-lg-4 text-center py-2">
                <div class="d-inline-flex align-items-center">
                    <i class="bi bi-geo-alt fs-1 text-primary me-3"></i>
                    <div class="text-start">
                        <h6 class="text-uppercase mb-1">Cửa hàng</h6>
                        <span>9A, Cửu Long, Nha Trang</span>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 text-center border-start border-end py-2">
                <div class="d-inline-flex align-items-center">
                    <i class="bi bi-envelope-open fs-1 text-primary me-3"></i>
                    <div class="text-start">
                        <h6 class="text-uppercase mb-1">Email</h6>
                        <span>chinchunpetshop@gmail.com</span>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 text-center py-2">
                <div class="d-inline-flex align-items-center">
                    <i class="bi bi-phone-vibrate fs-1 text-primary me-3"></i>
                    <div class="text-start">
                        <h6 class="text-uppercase mb-1">Số điện thoại</h6>
                        <span>090 525 08 79</span>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Topbar End -->
    <!-- Navbar Start -->
    <nav class="navbar navbar-expand-lg bg-white navbar-light shadow-sm py-3 py-lg-0 px-3 px-lg-0">
        <a href="~/" class="navbar-brand ms-lg-5">
            <h1 class="m-0 text-uppercase text-dark fs-3 d-flex align-items-center"><i class="bi bi-shop fs-1 text-primary me-3"></i><span>ChinChun PetShop</span></h1>
        </a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarCollapse">
            <div class="navbar-nav ms-auto py-0">
                <a href="~/" class="nav-item nav-link d-flex align-items-center @(ViewBag.active == "TrangChu"?"active":"")">Trang chủ</a>
                <a href="~/gioithieu" class="nav-item d-flex align-items-center nav-link @(ViewBag.active == "GioiThieu"?"active":"")">Giới thiệu</a>
                <a href="~/sanpham" class="nav-item d-flex align-items-center nav-link  @(ViewBag.active == "SanPham"?"active":"")">Sản phẩm</a>
                <a href="~/lienhe" class="nav-item d-flex align-items-center nav-link @(ViewBag.active == "LienHe"?"active":"")">Liên hệ</a>
                @if (user != null)
                {
                    <div class="nav-item dropdown me-3 d-flex justify-content-center">
                        <a class="nav-link dropdown-toggle" href="javascript:void(0)" data-bs-toggle="dropdown"
                           aria-haspopup="true" aria-expanded="false">
                            <img src="~/@(string.IsNullOrEmpty(user.HinhAnh)? "contents/nhanvien/assets/images/users/profile-pic.jpg":"images/khachhang/"+ user.HinhAnh)" alt="user" class="rounded-circle"
                                 width="40">
                            <span class="ms-2 d-none d-lg-inline-block">
                                <span>Chào,</span>
                                <span class="text-dark">@user.HoTen.</span>
                                <i data-feather="chevron-down" class="svg-icon"></i>
                            </span>
                        </a>
                        <div class="dropdown-menu dropdown-menu-end dropdown-menu-right user-dd animated flipInY">
                            <a class="dropdown-item" href="~/khachhang">
                                <i data-feather="user"
                                   class="svg-icon me-2 ms-1"></i>
                                Hồ sơ cá nhân
                            </a>
                            <div class="dropdown-divider"></div>
                            <a class="dropdown-item" href="~/khachhang/giohang">
                                <i data-feather="shopping-cart"
                                   class="svg-icon me-2 ms-1"></i>
                                Giỏ hàng
                            </a>
                            <div class="dropdown-divider"></div>
                            <a class="dropdown-item" href="~/khachhang/dondadat">
                                <i data-feather="shopping-cart"
                                   class="svg-icon me-2 ms-1"></i>
                                Đơn hàng đã đặt
                            </a>
                            <div class="dropdown-divider"></div>
                            <a class="dropdown-item" href="~/khachhang/caidat">
                                <i data-feather="settings"
                                   class="svg-icon me-2 ms-1"></i>
                                Cài đặt tài khoản
                            </a>
                            <div class="dropdown-divider"></div>
                            <a class="dropdown-item" href="~/khachhang/dangxuat">
                                <i data-feather="power"
                                   class="svg-icon me-2 ms-1"></i>
                                Đăng xuất
                            </a>

                        </div>
                    </div>
                }
                else
                {
                    <div class="nav-item dropdown me-3">
                        <a class="nav-link dropdown-toggle @(ViewBag.active == "TaiKhoan"?"active":"")" href="javascript:void(0)" data-bs-toggle="dropdown"
                           aria-haspopup="true" aria-expanded="false">
                            Tài khoản
                        </a>
                        <div class="dropdown-menu dropdown-menu-end dropdown-menu-right user-dd animated flipInY">
                            <a class="dropdown-item" href="~/khachhang/dangnhap">
                                Đăng nhập
                            </a>
                            <div class="dropdown-divider"></div>
                            <a class="dropdown-item" href="~/khachhang/dangky">
                                Đăng ký
                            </a>
                        </div>
                    </div>
                   
                   
                }

            </div>
        </div>
    </nav>
    <!-- Navbar End -->
    @RenderBody()
    @if (!string.IsNullOrWhiteSpace(ViewBag.message))
    {
        <div class="custom-alert">
            <div class="alert alert-success alert-dismissible my-alert my-alert-dismissible fade show" role="alert">
                @ViewBag.message
                <button type="button" class="btn-close fs-6" data-bs-dismiss="alert" aria-label="Close"></button>
            </div>
        </div>
    }
    @if (!string.IsNullOrWhiteSpace(ViewBag.TB))
    {
        <div class="custom-alert">
            <div class="alert alert-success alert-dismissible my-alert my-alert-dismissible fade show" role="alert">
                @ViewBag.TB
                <button type="button" class="btn-close fs-6" data-bs-dismiss="alert" aria-label="Close"></button>
            </div>
        </div>
    }

    <div class="modal fade" id="ThemVaoGioHang" tabindex="-1" aria-labelledby="ThemVaoGioHang" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" style="max-width:31rem">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="addToCartLabel">Thêm sản phẩm vào giỏ hàng</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                    <!-- Product Info -->
                    <div class="d-flex align-items-center mb-3">
                        <img id="productImage" src="" alt="Product" class="me-3" style="width: 80px; height: 130px; object-fit: cover;">
                        <div>
                            <h6 id="productName" class="mb-1">Tên sản phẩm: </h6>
                            <p>Nhãn hiệu: <span id="productNhanHieu" class="mb-0 text-muted"></span></p>
                        </div>
                    </div>
                    <!-- Form -->
                    <form id="addToCartForm" method="post" action="~/khachhang/giohang/them">
                        <input type="hidden" id="MaSP" name="MaSP" value="">
                        <div class="row d-flex justify-content-center">
                            <div class="mb-3 col-sm-6 product-qty-order text-center">
                                <label class="mb-2 d-block">Số lượng</label>
                                <div class="input-group mb-3 product-quantity" style="width: 170px;">
                                    <button class="btn btn-white border border-secondary px-3 quantity-left-minus m-auto" type="button" data-type="minus" data-mdb-ripple-color="dark">
                                        <i class="fas fa-minus"></i>
                                    </button>
                                    <input type="text" id="quantity" min="1" max="" class="form-control text-center border border-secondary quantity-order-change" value="1" name="SoLuong" aria-label="Số lượng sản phẩm" aria-describedby="button-addon1" />
                                    <button class="btn btn-white border border-secondary px-3 quantity-right-plus" type="button" data-type="plus" data-mdb-ripple-color="dark">
                                        <i class="fas fa-plus"></i>
                                    </button>
                                </div>
                                
                            </div>
                        </div>


                        <input type="hidden" id="pnext" name="Next" value="~/">
                        <div class="modal-footer justify-content-center">
                            <button type="submit" class="btn btn-primary" id="confirmAddToCart">Xác nhận</button>
                        </div>
                    </form>
                </div>

            </div>
        </div>
    </div>

    <!-- Footer Start -->
    <div class="container-fluid bg-light mt-5 py-5">
        <div class="container pt-5">
            <div class="row g-5">
                <div class="col-lg-6 col-md-6">
                    <h5 class="text-uppercase border-start border-5 border-primary ps-3 mb-4">Liên hệ</h5>
                    <p class="mb-4">Chuyên cung cấp cát vệ sinh, thức ăn, phụ kiện cho thú cưng</p>
                    <p class="mb-2"><i class="bi bi-geo-alt text-primary me-2"></i>9A, Cửu Long, Nha Trang</p>
                    <p class="mb-2"><i class="bi bi-envelope-open text-primary me-2"></i>chinchunpetshop@gmail.com</p>
                    <p class="mb-0"><i class="bi bi-telephone text-primary me-2"></i>090 525 08 79</p>
                </div>
                <div class="col-lg-3 col-md-6">
                    <h5 class="text-uppercase border-start border-5 border-primary ps-3 mb-4">Liên kết</h5>
                    <div class="d-flex flex-column justify-content-start">
                        <a class="text-body mb-2" href="#"><i class="bi bi-arrow-right text-primary me-2"></i>Trang chủ</a>
                        <a class="text-body mb-2" href="#"><i class="bi bi-arrow-right text-primary me-2"></i>Giới thiệu</a>
                        <a class="text-body mb-2" href="#"><i class="bi bi-arrow-right text-primary me-2"></i>Sản phẩm</a>
                        <a class="text-body" href="#"><i class="bi bi-arrow-right text-primary me-2"></i>Liên hệ</a>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6">
                    <h5 class="text-uppercase border-start border-5 border-primary ps-3 mb-4">Theo dõi</h5>
                    <div class="d-flex">
                        <a class="btn btn-outline-primary btn-square me-2" href="https://www.facebook.com/chinchunpetshop"><i class="bi bi-facebook"></i></a>
                        <a class="btn btn-outline-primary btn-square" href="https://www.instagram.com/chinchunpetshop/"><i class="bi bi-instagram"></i></a>
                    </div>
                </div>
                <div class="col-12 text-center text-body">
                    <a class="text-body" href="">Điều khoản sử dụng</a>
                    <span class="mx-1">|</span>
                    <a class="text-body" href="">Chính sách</a>
                    <span class="mx-1">|</span>
                    <a class="text-body" href="">Hỗ trợ khách hàng</a>
                    <span class="mx-1">|</span>
                    <a class="text-body" href="">Thanh toán</a>
                    <span class="mx-1">|</span>
                    <a class="text-body" href="">Giúp đỡ</a>
                    <span class="mx-1">|</span>
                    <a class="text-body" href="">Câu hỏi thường gặp</a>
                </div>
            </div>
